{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "contact": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "location": {
          "type": "string"
        },
        "email": {
          "type": "string",
          "format": "email"
        },
        "phone": {
          "type": "string"
        },
        "social": {
          "type": "array",
          "items": {
            "type": "object",
            "patternProperties": {
              "^.+$": {
                "type": "string",
                "format": "uri"
              }
            },
            "additionalProperties": false
          }
        }
      },
      "required": [
        "name",
        "email"
      ]
    },
    "summary": {
      "type": "string"
    },
    "competencies": {
      "type": "string"
    },
    "experiences": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "company": {
            "type": "string"
          },
          "stack": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "date": {
            "type": "object",
            "properties": {
              "start": {
                "type": "string",
                "pattern": "^\\d{4}/\\d{2}$"
              },
              "end": {
                "type": [
                  "string",
                  "null"
                ],
                "pattern": "^\\d{4}/\\d{2}$"
              }
            },
            "required": [
              "start"
            ]
          },
          "content": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "bullets": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "title",
          "company",
          "date"
        ]
      }
    },
    "education": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "institution": {
            "type": "string"
          },
          "date": {
            "type": "object",
            "properties": {
              "start": {
                "type": "integer"
              },
              "end": {
                "type": "integer"
              }
            },
            "required": [
              "start",
              "end"
            ]
          },
          "speciality": {
            "type": "string"
          }
        },
        "required": [
          "title",
          "institution",
          "date"
        ]
      }
    },
    "languages": {
      "type": "array",
      "items": {
        "type": "object",
        "minProperties": 1,
        "maxProperties": 1,
        "additionalProperties": {
          "type": "string"
        }
      }
    },
    "interests": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": [
    "contact",
    "competencies",
    "experiences",
    "education"
  ]
}
